{
  "executable": "CharacterManager",
  "splashscreen": {
    "imageFile": "Assets/Splash.png"
  },
  "name": "CharacterManager",
  "author": "Andrew Ettensohn",
  "singleInstance": false,
  "build": {
    "appId": "com.CharacterManager.app",
    "productName": "CharacterManager",
    "copyright": "Copyright Â© 2021",
    "buildVersion": "0.9.0",
    "compression": "maximum",
    "win": {
      "icon": "Assets/icon.png"
    },
    "directories": {
      "output": "../../../bin/Desktop",
      "buildResources": "Assets"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}